{
  "presets": [
    [
      "env",
      {
        "exclude": [
          "transform-es2015-classes"
        ]
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "jest",
        [
          "env",
          {
            "exclude": [
              "transform-es2015-classes"
            ]
          }
        ]
      ]
    }
  },
  "ignore": [],
  "plugins": [
    [
      "babel-plugin-transform-builtin-extend",
      {
        "globals": [
          "Error",
          "Array"
        ]
      }
    ]
  ]
}
